    "import numpy as np\n",
    "from numpy.random import default_rng\n",
    "\n",
    "# Create a reproducible random number generator\n",
    "rng = default_rng(12345)\n",
    "\n",
    "# Generate 100 random ages between 1 and 100 (inclusive)\n",
    "random_ages = rng.integers(1, 101, size=100)\n",
    "\n",
    "print(random_ages)"
